    "ping:start": "node scripts/render-ping.js",
    "logs:list": "node scripts/log-manager.js list",
    "logs:analyze": "node scripts/log-manager.js analyze",
    "logs:search": "node scripts/log-manager.js search",
    "logs:watch": "node scripts/log-manager.js watch",
    "logs:backup": "node scripts/log-manager.js backup",
    "logs:report": "node scripts/log-manager.js report",
