{
  "name": "logawa-discord-logger",
  "version": "1.0.0",
  "description": "A comprehensive Discord logging bot that tracks messages, moderation actions, and server events",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
